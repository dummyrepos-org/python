   ],
   "source": [
    "n1 = 1\n",
    "n2 = 2\n",
    "result = 0\n",
    "total = 0\n",
    "while n2 < 4000000:\n",
    "    result = n1 + n2\n",
    "    if n2 % 2 == 0:\n",
    "        total += n2\n",
    "    n1 = n2\n",
    "    n2 = result\n",
    "print(total)"
   ]
